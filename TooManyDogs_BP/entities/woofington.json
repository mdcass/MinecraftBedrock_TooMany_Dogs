{
    "format_version": "1.20.0",
    "minecraft:entity": {
      "description": {
        "identifier": "tmd:woofington",
        "is_spawnable": true,
        "is_summonable": true,
        "is_experimental": false
      },
      "components": {
        "minecraft:type_family": { "family": ["npc", "villager"] },
        "minecraft:nameable": {
          "allow_name_tag_renaming": false,
          "always_show": true
        },
        "minecraft:health": { "value": 20, "max": 20 },
        "minecraft:navigation.walk": { "can_path_over_water": true },
        "minecraft:interact": {
          "interactions": [
            {
              "on_interact": {
                "filters": {
                  "all_of": [
                    { "test": "is_family", "subject": "other", "value": "player" }
                  ]
                },
                "target": "self",
                "event": "tmd:woof_interact"
              },
              "interact_text": "Talk"
            }
          ]
        }
      },
      "events": {
        "tmd:woof_interact": {
          "sequence": [
            {
              "queue_command": {
                "target": "self",
                "command": "function tmd:woof_interact_handler"
              }
            }
          ]
        }
      }
    }
  }
  